query TeamMembersByOrganization($organization: String!, $team: String!) {
  organization(login: $organization) {
    teams(query: $team, first:20) {
      edges {
        node {
          name
          members(first: 20) {
            edges {
              node {
                name
                pullRequests(first: 20, states: OPEN) {
                  edges {
                    node {
                      title
                      createdAt
                      id
                      url
                     
                    }
                  }
                }
              }
            }
          }
        }
      }
    }
  }
}



